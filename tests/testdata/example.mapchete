# mandatory parameters
process: mapchete.processes.examples.example_process
bounds: [2.0, 1.0, 3.0, 4.0]
zoom_levels:
    min: 7
    max: 11
pyramid:
    grid: geodetic
    metatiling: 4
input:
    file1: https://ungarj.github.io/mapchete_testdata/tiled_data/raster/cleantopo/1/0/0.tif
    file2: https://ungarj.github.io/mapchete_testdata/tiled_data/raster/cleantopo/1/0/1.tif
output:
    path: tmp
    format: GTiff
    dtype: float32
    bands: 3

# free parameters
some_integer_parameter: 12
some_float_parameter: 5.3
some_string_parameter:
    zoom<=7: string1
    zoom>7: string2
some_bool_parameter: true
some_date_obj: 2018-05-01



# mhub parameters:
mhub_worker: execute_worker
mhub_queue: execute_worker_queue
mhub_next_process:
    mhub_mode: overwrite
    mhub_worker: execute_worker
    process: mapchete.processes.examples.example_process
    bounds: [3.0, 1.0, 4.0, 2.0]
    zoom_levels:
        min: 7
        max: 11
    pyramid:
        grid: geodetic
        metatiling: 4
    input:
        file1: https://ungarj.github.io/mapchete_testdata/tiled_data/raster/cleantopo/1/0/0.tif
        file2: https://ungarj.github.io/mapchete_testdata/tiled_data/raster/cleantopo/1/0/1.tif
    output:
        path: test
        format: GTiff
        dtype: float32
        bands: 1

    mhub_next_process:
        mhub_worker: index_worker
        process: mapchete.processes.examples.example_process
        zoom_levels:
            min: 7
            max: 11
        pyramid:
            grid: geodetic
            metatiling: 4
        input:
        output:
            path: test
            format: GTiff
            dtype: float32
            bands: 1
