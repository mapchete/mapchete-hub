# mhub batch s2cloudless.mhub --bounds 1 2 3 4 --jobs overviews

jobs:

  mosaic:
    command: execute
    mapchete: example.mapchete
    zoom: 11

  overviews:
    command: execute
    queue: overview_queue
    mapchete: example.mapchete
    zoom: [7, 10]
    mode: overwrite

  indexes:
    command: index
    mapchete: example.mapchete
    announce_on_slack: true
