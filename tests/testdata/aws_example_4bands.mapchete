process: mapchete_hub.processes.extract_mosaic
input:
  primary:
    format: S2AWS
    start_time: 2018-04-02
    end_time: 2018-04-06
    with_cloudmasks: true
    cache:
      path: cache
      bands: [4,3,2,8]
      keep: true
      intersection_percent: 0
output:
  format: GTiff
  bands: 4
  path: tmp/aws_example_4bands
  dtype: uint8
pyramid:
  grid: geodetic
zoom_levels: 13
bounds: [8.02001953125, 46.34033203125, 8.0419921875, 46.3623046875]

bands: [4, 3, 2, 8]